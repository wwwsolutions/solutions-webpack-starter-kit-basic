{
  "presets": [
    [
      "@babel/preset-env",
      {
        "debug": false,
        "loose": true,
        "modules": false,
        "useBuiltIns": "entry",
        "corejs": 3,
        "targets": {
          "node": "current",
          "esmodules": true
        }
      }
    ]
  ],
  "plugins": [
    [
      "@babel/plugin-transform-runtime",
      {
        "corejs": false,
        "helpers": true,
        "regenerator": true,
        "useESModules": true
      }
    ],
    [
      "@babel/plugin-transform-modules-commonjs"
    ],
    [
      "@babel/plugin-syntax-dynamic-import"
    ],
    [
      "@babel/plugin-transform-regenerator"
    ],
    [
      "@babel/plugin-proposal-class-properties"
    ]
  ]
}