{
    "workbench.colorCustomizations": {
        "activityBar.background": "#2E1C74",
        "titleBar.activeBackground": "#4128A3",
        "titleBar.activeForeground": "#FBFBFE"
    },
    "css.validate": false,
    "less.validate": false,
    "scss.validate": false,
    "files.associations": {
        "*.css": "scss"
    },
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
        "source.fixAll.stylelint": true
    },
    "files.autoSaveDelay": 500,
    "cSpell.ignoreWords": [
        "fscale",
        "plugable"
    ]
}